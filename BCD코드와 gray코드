int main()
{
	string array;
	cin >> array;
	//cout << size(array) << endl;
	int *changed_num = new int[sizeof(array)];
	int* gray_code = new int[sizeof(array)];
	int word = size(array);
	for (int j = 0; j < word; j++)
	{
		array[j] = int(array[j] - 48);
	}
	changed_num[0] = array[0];
	
	for (int i = 1; i < word; i++)
	{
		//cout << "Word" << word - 1 << endl;
		if (array[i] == array[i - 1])
		{
			changed_num[i] = 0;
		}
		else
		{
			changed_num[i] = 1;
		}
	}

	for (int k = 0; k < word; k++)
	{
		cout <<changed_num[k];
		if (array[k] == changed_num[k])
		{
			gray_code[k] = 0;
		}
		else
			gray_code[k] = 1;
		
	}
	cout << endl;
	for (int b = 0; b < word; b++)
	{
		cout<< gray_code[b];
	}
	
}
