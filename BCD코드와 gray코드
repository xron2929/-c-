int main()
{
#include<iostream>
using namespace std;


void gray(char a[100],int n)
{
	cout<<a[0]<<" ";
	for(int i=1;i<n;i++)
	{
		if(a[i]==a[i-1]) cout<<0<<" ";
		else if(a[i]!=a[i-1]) cout<<1<<" ";
	}
}

int main()
{
	char a[100];
	int n;
	cin>>n;
	for(int i=0;i<n;i++)
	{
		cin>>a[i];
	}
	gray(a,n);
}
	
}
